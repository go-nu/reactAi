type Employee {
    id: ID!
    name: String!
    age: Int!
    job: String!
    language: String!
    pay: Int!
}

input EmployeeInput {
    name: String!
    age: Int!
    job: String!
    language: String!
    pay: Int!
}

type Query {
    employee(id: ID!): Employee
}

type Mutation {
    createEmployee(input: EmployeeInput!): Employee!
    udateEmployee(id: ID!, input: EmployeeInput!): Employee!
    deleteEmployee(id: ID!): Boolean!
}


